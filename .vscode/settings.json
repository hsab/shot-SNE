{
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "files.exclude":{
        "**/.readme": true,
        "**/.directory": true,
        "**/.appveyor.yml": true,
        "**/.gitignore": true,
        "**/.travis.yml": true
    },
    "spellright.language": "English (American)",
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "files.associations": {
        "iostream": "cpp",
        "bitset": "cpp",
        "typeinfo": "cpp",
        "condition_variable": "cpp",
        "mutex": "cpp"
    }
}